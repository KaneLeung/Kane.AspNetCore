{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "jwt": {
    "Secret": "123456", //JWT密钥，小于16位，则取其MD5值
    "Issuer": "Hello", //发行方
    "Audience": "World", //订阅方
    "Expires": 1, //过期时间1分钟
    "Refresh": 20 //刷新令牌时间，一般比过期时间长
  }
}